<?xml version="1.0"?>
<project name="WebPoker" default="build">
  <presetdef name="javac">
    <javac includeantruntime="false" />
  </presetdef>

  <target name="build" description="Compile sources">
    <mkdir dir="./bin/"/>
    <javac srcdir="./src/" destdir="./bin/" debug="true" failonerror="true"/>
  </target>
 
  <target name="clean" description="Clean output files">
    <delete dir="./bin/"/>
  </target>
</project>